{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/locations/search",
      "dest": "dist/index.js",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
} 